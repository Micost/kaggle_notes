     "text": [
      "{'prompt': '{\"A\":30,\"op\":\"/\",\"B\":24}', 'chosen': ['{\"result\": \"1.25\"}'], 'rejected': ['{\"result\": \"0.125\"}']}\n",
      "{'prompt': '{\"A\":81,\"op\":\"*\",\"B\":94}', 'chosen': ['{\"result\": \"7614\"}'], 'rejected': ['{\"result\": \"60912\"}']}\n",
      "{'prompt': '{\"A\":83,\"op\":\"*\",\"B\":98}', 'chosen': ['{\"result\": \"8134\"}'], 'rejected': ['{\"result\": \"56938\"}']}\n",
      "{'prompt': '{\"A\":91,\"op\":\"+\",\"B\":79}', 'chosen': ['{\"result\": \"170\"}'], 'rejected': ['{\"result\": \"173\"}']}\n",
      "{'prompt': '{\"A\":86,\"op\":\"+\",\"B\":63}', 'chosen': ['{\"result\": \"149\"}'], 'rejected': ['{\"result\": \"154\"}']}\n"
     ]
    }
   ],
   "source": [
    "import random\n",
    "\n",
    "def calculate(op, A, B):\n",
    "  if op == '+':\n",
    "    return A + B\n",
    "  elif op == '-':\n",
    "    return A - B\n",
    "  elif op == '*':\n",
    "    return A * B\n",
    "  elif op == '/':\n",
    "    if B == 0:\n",
    "      return \"Division by zero\"\n",
    "    else:\n",
    "      return A / B\n",
    "  else:\n",
    "    return \"Invalid operator\"\n",
    "\n",
    "def generate_dataset(num_samples):\n",
    "  dataset = []\n",
    "  operators = ['+', '-', '*', '/']\n",
    "  for _ in range(num_samples):\n",
    "    op = random.choice(operators)\n",
    "    A = random.randint(1, 100)\n",
    "    B = random.randint(1, 100)\n",
    "    C = random.randint(2, 10)\n",
    "    result = calculate(op, A, B)\n",
    "    wrong_result = calculate(op, result,C)\n",
    "    data = {\n",
    "        \"prompt\": f'{{\"A\":{A},\"op\":\"{op}\",\"B\":{B}}}',\n",
    "        \"chosen\": [ fr'{{\"result\": \"{result}\"}}' ],\n",
    "        \"rejected\": [ fr'{{\"result\": \"{wrong_result}\"}}' ]\n",
    "    }\n",
    "    dataset.append(data)\n",
    "  return dataset\n",
    "\n",
    "\n",
    "data = generate_dataset(1000)\n",
    "\n",
    "\n",
    "for item in data[:5]:\n",
    "  print(item)"
   ]
  },
